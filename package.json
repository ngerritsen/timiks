{
  "name": "timiks",
  "version": "1.0.0",
  "description": "Rubik's cube timer",
  "main": "index.js",
  "scripts": {
    "static": "mkdir -p public; cp -r src/static/* public/; cp -r src/sounds public/; cp -r src/fonts public/",
    "images": "mkdir -p public/images; imagemin src/images/* --out-dir public/images",
    "copy-images": "mkdir -p public/images; cp src/images/* public/images/",
    "watch": "npm run static; npm run copy-images; webpack-dev-server --mode=development --content-base ./public",
    "test": "eslint .",
    "build": "npm run static; npm run images; webpack --mode=production",
    "build-number": "echo \"$TRAVIS_BUILD_NUMBER\" > src/static/build.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ngerritsen/timiks.git"
  },
  "keywords": [
    "rubiks",
    "cube",
    "timer"
  ],
  "author": "Niels Gerritsen",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ngerritsen/timiks/issues"
  },
  "homepage": "https://github.com/ngerritsen/timiks#readme",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-brands-svg-icons": "^5.13.0",
    "@fortawesome/free-regular-svg-icons": "^5.14.0",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "@sentry/browser": "^5.9.1",
    "chart.js": "^2.9.4",
    "chartjs-plugin-zoom": "^0.7.5",
    "connected-react-router": "^6.6.0",
    "element-closest": "^3.0.2",
    "firebase": "^7.5.0",
    "imagemin": "^7.0.1",
    "imagemin-cli": "^5.0.0",
    "keycode": "^2.2.0",
    "nosleep.js": "^0.9.0",
    "polished": "^3.4.2",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-chartjs-2": "^2.8.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-spring": "^8.0.27",
    "redux": "^4.0.4",
    "redux-actions": "^2.6.5",
    "redux-logger": "^3.0.6",
    "redux-observable": "^1.2.0",
    "reselect": "^4.0.0",
    "rxjs": "^6.5.3",
    "shortid": "^2.2.15",
    "styled-components": "^4.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "eslint": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.1.0",
    "prettier": "^1.19.1",
    "uglifycss": "0.0.29",
    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "browsers": [
              "last 2 versions"
            ]
          }
        }
      ],
      "@babel/preset-react"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test"
    }
  }
}
