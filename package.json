{
  "name": "timiks",
  "version": "1.0.0",
  "description": "Rubik's cube timer",
  "main": "index.js",
  "scripts": {
    "static": "mkdir -p public; cp -r src/static/* public/; cp -r src/sounds public/; cp -r src/fonts public/",
    "images": "mkdir -p public/images; imagemin src/images/* --out-dir public/images",
    "copy-images": "mkdir -p public/images; cp src/images/* public/images/",
    "watch": "npm run static; npm run copy-images; webpack serve --mode=development",
    "test": "eslint .; prettier --check .",
    "fix": "eslint . --fix; prettier --write .",
    "build": "npm run static; npm run images; webpack --mode=production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/ngerritsen/timiks.git"
  },
  "keywords": [
    "rubiks",
    "cube",
    "timer"
  ],
  "author": "Niels Gerritsen",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/ngerritsen/timiks/issues"
  },
  "homepage": "https://gitlab.com/ngerritsen/timiks#readme",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-brands-svg-icons": "^5.13.0",
    "@fortawesome/free-regular-svg-icons": "^5.14.0",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "@sentry/browser": "^5.9.1",
    "chart.js": "^2.9.3",
    "chartjs-plugin-zoom": "^0.7.5",
    "connected-react-router": "^6.6.0",
    "element-closest": "^3.0.2",
    "firebase": "^7.5.0",
    "imagemin": "^7.0.1",
    "imagemin-cli": "^5.0.0",
    "keycode": "^2.2.0",
    "nosleep.js": "^0.9.0",
    "polished": "^4.2.2",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-chartjs-2": "^2.8.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.2.9",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-spring": "^8.0.27",
    "redux": "^4.2.1",
    "redux-actions": "^2.6.5",
    "redux-logger": "^3.0.6",
    "redux-observable": "^2.0.0",
    "reselect": "^4.1.8",
    "rxjs": "^7.8.1",
    "shortid": "^2.2.15",
    "style-components": "^0.1.0",
    "styled-components": "^4.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.21.5",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "babel-loader": "^9.1.2",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react": "^7.32.2",
    "html-webpack-plugin": "^5.5.1",
    "husky": "^8.0.3",
    "prettier": "^2.8.8",
    "uglifycss": "0.0.29",
    "webpack": "^5.81.0",
    "webpack-bundle-analyzer": "^4.8.0",
    "webpack-cli": "^5.0.2",
    "webpack-dev-server": "^4.13.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test"
    }
  }
